{
  "version": 1,
  "projects": {
    "pending-transactions": {
      "root": "apps/pending-transactions",
      "sourceRoot": "apps/pending-transactions/src",
      "projectType": "application",
      "prefix": "pending-transactions",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/pending-transactions",
            "main": "apps/pending-transactions/src/main.ts",
            "tsConfig": "apps/pending-transactions/tsconfig.app.json",
            "assets": ["apps/pending-transactions/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/pending-transactions/src/environments/environment.ts",
                  "with": "apps/pending-transactions/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "pending-transactions:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/pending-transactions/.eslintrc",
            "tsConfig": [
              "apps/pending-transactions/tsconfig.app.json",
              "apps/pending-transactions/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/pending-transactions/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/pending-transactions/jest.config.js",
            "tsConfig": "apps/pending-transactions/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "utils": {
      "root": "libs/utils",
      "sourceRoot": "libs/utils/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "libs/utils/.eslintrc",
            "tsConfig": [
              "libs/utils/tsconfig.lib.json",
              "libs/utils/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/utils/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/utils/jest.config.js",
            "tsConfig": "libs/utils/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "file-storage": {
      "root": "libs/file-storage",
      "sourceRoot": "libs/file-storage/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "libs/file-storage/.eslintrc",
            "tsConfig": [
              "libs/file-storage/tsconfig.lib.json",
              "libs/file-storage/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/file-storage/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/file-storage/jest.config.js",
            "tsConfig": "libs/file-storage/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/node"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "linter": "eslint",
        "babel": true
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "styled-components",
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "pending-transactions"
}
